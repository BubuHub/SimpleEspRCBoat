<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <com.elfin.Boat.ProgressImageWievH
        android:id="@+id/touchValue1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="24dp"
        android:layout_x="0dp"
        android:layout_y="16dp" />

    <com.elfin.Boat.ProgressImageWiev
        android:id="@+id/touchValue2"
        android:layout_width="40dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/touchValue1"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="80dp" />

    <com.elfin.Boat.ProgressImageWiev
        android:id="@+id/progressImageWiev1"
        android:layout_width="20dp"
        android:layout_height="84dp"
        android:layout_alignBottom="@+id/touchValue2"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="36dp" />

    <com.elfin.Boat.VerticalTextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/progressImageWiev1"
        android:layout_alignTop="@+id/progressImageWiev1"
        android:layout_marginLeft="22dp"
        android:layout_toRightOf="@+id/progressImageWiev1"
        android:gravity="center"
        android:text="-"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <com.elfin.Boat.VerticalTextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView1"
        android:layout_alignTop="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:gravity="center"
        android:text="-"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <com.elfin.Boat.DirButton
        android:id="@+id/dirButton1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/touchValue1"
        android:layout_below="@+id/touchValue1"
        android:layout_marginTop="40dp" />

</RelativeLayout>